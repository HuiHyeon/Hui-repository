#include "stdafx.h"

int main(void)
{
   int x, y, z, result;
   
   printf("정수를 입력하시오 :");
   scanf_s("%d", &x);
   
   printf("정수를 입력하시오 :");
   scanf_s("%d", &y);
   
   printf("정수를 입력하시오 :");
   scanf_s("%d", &z);
   
   result = (x + y + z) / 3;
   
   printf("평균은 %d입니다.\n", result);
   
   return 0;
}
